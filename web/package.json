{
  "name": "radiance-wellness",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "web": "turbo dev --filter=@radiance/web",
    "client": "turbo dev --filter=@radiance/client",
    "admin": "turbo dev --filter=@radiance/admin",
    "mobile": "turbo dev --filter=@radiance/mobile"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18"
  }
}
